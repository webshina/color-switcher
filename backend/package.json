{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "nodemon -r ./ts-node-require.ts src/app.ts",
    "db:migrate": "dotenv -e .env.development.local -- npx prisma migrate dev",
    "db:migrate:reset": "dotenv -e .env.development.local -- npx prisma migrate reset",
    "db:seed": "dotenv -e .env.development -- dotenv -e .env.development.local -- ts-node --require ./ts-node-require.ts seeds/index.ts",
    "db:sample": "dotenv -e .env.development -- dotenv -e .env.development.local -- ts-node --require ./ts-node-require.ts seeds/sampleData/index.ts",
    "exec:poc": "dotenv -e .env.development -- dotenv -e .env.development.local -- ts-node --require ./ts-node-require.ts src/poc/test.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.352.0",
    "@prisma/client": "^4.15.0",
    "@types/node": "^20.2.5",
    "axios": "^1.4.0",
    "cld3-asm": "^3.1.1",
    "cookie": "^0.5.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "discord.js": "^14.11.0",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "formidable": "^2.1.1",
    "iso-639-1": "^2.1.15",
    "js-cookie": "^3.0.5",
    "mime-types": "^2.1.35",
    "nodemon": "^2.0.22",
    "openai": "^3.2.1",
    "prisma": "^4.15.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "devDependencies": {
    "@types/cookie": "^0.5.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/formidable": "^2.0.6",
    "@types/langs": "^2.0.1",
    "@types/mime-types": "^2.1.1"
  }
}